<table class="table table-striped">
    <thead>
        <tr>
            <th>Codigo</th>
            <th>Nombre</th>
            <th>Precio</th>
            <th>Estado</th>
            <th>Disponible</th>
            <th>Puntuación</th>
            <th>Salud</th>
        </tr>
    </thead>
    <tbody id="order-list-container">
        @if (productList != null)
        {

            @foreach (Product prod in productList)
            {
                <label>@prod.Name</label>
            }
        }
    </tbody>
</table>

@code{

    [ParameterAttribute]
    public IEnumerable<Product>? productList { get; set; }
}